{
    "name": "linio/tortilla",
    "description": "A lightweight framework built as middleware, creating the base application to be passed to the stack.",
    "keywords": ["linio", "api", "framework", "tortilla", "stack", "stackphp", "middleware"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.6",
        "symfony/http-foundation": "~2.7",
        "symfony/http-kernel": "~2.7",
        "symfony/event-dispatcher": "~2.7",
        "nikic/fast-route": "0.6.*",
        "pimple/pimple": "~3.0",
        "linio/common": "~1.1",
        "linio/util": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Linio\\Tortilla\\": "src"
        }
    }
}
